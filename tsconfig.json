{
  "compilerOptions": {
    "removeComments": true,
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "module": "ESNext",
    "target": "ESNext",
    "typeRoots": ["./node_modules/@altv", "./node_modules/altv-esbuild/altv-enums"]
  },
  "include": ["./resources/test/src/**/*", "./*.d.ts"]
}
